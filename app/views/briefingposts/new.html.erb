<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "Go Show")}", routings_path  %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.list", :default => "Briefings")}", briefingposts_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title">New Briefing Note</h2>
    <div class="inner">
      <%= form_for :briefingpost, :url => briefingposts_path, :html => {:id => 'code_search', :name => 'code_search', :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>