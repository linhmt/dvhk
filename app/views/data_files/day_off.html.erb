<div class="block">
  <%= render(:partial => 'secondary') %>
  <div class="content">
    <h2 class="title">Load Monthly Day Off</h2>
    <div class="inner">
      <%= form_for @data_file, :url => create_day_off_data_files_path, :html => {:multipart => true, :class => :form } do |f| -%>
        <div class="group">
          <%= f.label :dailyroster, "Filename", :class => :label%>
          <%= f.file_field :dailyroster %>
        </div>
        <div class="group">
          <%= f.label :active_date, "Month - Year", :class => :label%>
          <%= f.date_select :active_date, :discard_day => true, :order => [:month, :year]  %>
        </div>
        <div class="group navform wat-cf">
          <button class="button" type="submit">
            <%= image_tag("web-app-theme/icons/tick.png", :alt => "Save")%>Save
          </button>
          <%= img_link_tag 'Cancel', 'cross.png', :href => shift_trackings_path, :class => 'button' %>
        </div>
      <% end -%>
    </div>
  </div>
</div>
<% content_for :sidebar, render(:partial => 'shared/sidebar') -%>