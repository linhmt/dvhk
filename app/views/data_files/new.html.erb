<div class="block">
  <%= render(:partial => 'secondary') %>
  <div class="content">
    <h2 class="title">New File Upload</h2>
    <div class="inner">
      <%= form_for @data_file, :url => data_files_path, :html => {:multipart => true, :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'shared/sidebar') -%>