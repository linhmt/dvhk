<div class="block">
  <%= render(:partial => 'secondary', :locals => {:routing => @passenger.routing}) %>
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> Passenger</h2>
    <div class="inner">
      <%= form_for @passenger, :url => routing_passenger_path, :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'shared/routings') -%>