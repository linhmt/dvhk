<h1>Listing passengers</h1>

<table>
  <tr>
    <th>Pax name</th>
    <th>Personal</th>
    <th>Routing</th>
    <th>Remark</th>
    <th>Ticket class</th>
    <th>Priority</th>
    <th>Sequence</th>
    <th>Origin flt</th>
    <th>Accepted flt</th>
    <th>Accepted</th>
    <th>Called</th>
    <th>Is active</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @passengers.each do |passenger| %>
  <tr>
    <td><%= passenger.pax_name %></td>
    <td><%= passenger.personal_id %></td>
    <td><%= passenger.routing_id %></td>
    <td><%= passenger.remark %></td>
    <td><%= passenger.ticket_class %></td>
    <td><%= passenger.priority_id %></td>
    <td><%= passenger.sequence %></td>
    <td><%= passenger.origin_flt %></td>
    <td><%= passenger.accepted_flt %></td>
    <td><%= passenger.accepted %></td>
    <td><%= passenger.called %></td>
    <td><%= passenger.is_active %></td>
    <td><%= passenger.user_id %></td>
    <td><%= link_to 'Show', passenger %></td>
    <td><%= link_to 'Edit', edit_passenger_path(passenger) %></td>
    <td><%= link_to 'Destroy', passenger, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Passenger', new_passenger_path %>
