<div class="block">
  <%= render(:partial => 'secondary') %>
  <div class="content">
    <h2 class="title">
      Roles List
    </h2>
    <div class="inner">
      <ul>
        <% @roles.each do |role| %>
        <li><%= role.name %> - <%= role.resource_type %> </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
<% content_for :sidebar, render(:partial => 'sidebar') -%>