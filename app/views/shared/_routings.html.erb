<% @routings = Routing.where(:is_arrival => false).order('destination asc') %>
<%= render(:partial => 'shared/important_notice') %>
<div class="block">
  <h3>Routings</h3>
  <ul class="navigation">
    <% @routings.each do |routing| -%>
      <li><%= link_to routing.destination, routing_passengers_path(routing) %></li>
    <% end -%>
  </ul>
</div>