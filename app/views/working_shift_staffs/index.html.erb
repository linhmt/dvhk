<h1>Listing working_shift_staffs</h1>

<table class="table">
  <tr>
    <th>Roster</th>
    <th>Duty date</th>
    <th></th>
    <th></th>
  </tr>

<% @working_shift_staffs.each do |working_shift_staff| %>
  <tr>
    <td><%= sanitize working_shift_staff.roster %></td>
    <td><%= working_shift_staff.duty_date %></td>
    <td><%= link_to 'Edit', edit_working_shift_staff_path(working_shift_staff) %></td>
    <td><%= link_to 'Destroy', working_shift_staff, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

